#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/bin/css-beautify.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/js-beautify@1.15.1/node_modules/js-beautify/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/js-beautify@1.15.1/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/js-beautify/js/bin/css-beautify.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/js-beautify/js/bin/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/js-beautify/js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/js-beautify/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../js-beautify/js/bin/css-beautify.js" "$@"
else 
  exec node  "$basedir/../js-beautify/js/bin/css-beautify.js" "$@"
fi
