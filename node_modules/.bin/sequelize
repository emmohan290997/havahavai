#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/sequelize-cli@6.6.2/node_modules/sequelize-cli/lib/sequelize/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/sequelize-cli@6.6.2/node_modules/sequelize-cli/lib/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/sequelize-cli@6.6.2/node_modules/sequelize-cli/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/sequelize-cli@6.6.2/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/sequelize-cli/lib/sequelize/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/sequelize-cli/lib/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/sequelize-cli/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sequelize-cli/lib/sequelize" "$@"
else 
  exec node  "$basedir/../sequelize-cli/lib/sequelize" "$@"
fi
