#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/esm/bin.mjs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/esm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/glob@10.4.2/node_modules/glob/dist/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/glob@10.4.2/node_modules/glob/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/glob@10.4.2/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/glob/dist/esm/bin.mjs/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/glob/dist/esm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/glob/dist/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/Myapp/node_modules/glob/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../glob/dist/esm/bin.mjs" "$@"
else 
  exec node  "$basedir/../glob/dist/esm/bin.mjs" "$@"
fi
